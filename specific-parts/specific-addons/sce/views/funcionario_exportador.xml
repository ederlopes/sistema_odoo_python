<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Funcionario Exportador -->

    <!-- Form Funcionario Exportador -->
    <record id="sce_funcionario_exportador_view_form" model="ir.ui.view">
        <field name="name">sce.funcionario.exportador.view.form</field>
        <field name="model">funcionario.exportador</field>
        <field name="priority">16</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h1><field name="name_funcionario" nolabel="1" /></h1>
                    <group>
                         <field name="cpf_cnpj_funcionario" />
                         <field name="email_funcionario" />
                         <field name="mobile_funcionario" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
